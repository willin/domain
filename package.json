{
  "name": "free-domain",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "prepare": "husky install",
    "pages:build": "npx @cloudflare/next-on-pages --experimental-minify",
    "pages:deploy": "npm run pages:build && wrangler@3 pages publish .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npm run pages:build && npx wrangler@3 pages dev .vercel/output/static --kv=DOMAINS --compatibility-flag=nodejs_compat --compatibility-date=2023-05-12"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "dlv": "^1.1.3",
    "next": "13.4.2",
    "next-auth": "^4.22.1",
    "punycode": "^2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sqlstring": "^2.3.3",
    "templite": "^1.2.0",
    "theme-change": "^2.5.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.0.0",
    "@cloudflare/workers-types": "^4.20230511.0",
    "@formatjs/intl-localematcher": "^0.2.32",
    "@types/dlv": "^1.1.2",
    "@types/negotiator": "^0.6.1",
    "@types/node": "20.1.7",
    "@types/punycode": "^2.1.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/sqlstring": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.44.0",
    "autoprefixer": "10.4.14",
    "daisyui": "^2.51.6",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "negotiator": "^0.6.3",
    "postcss": "8.4.23",
    "prettier": "^2.7.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vercel": "^28.20.0",
    "wrangler": "^2.20.0"
  }
}
